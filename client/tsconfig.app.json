{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "baseStyles.ts", "../types/*"],
  "exclude": ["./src/**/*.spec.*","./src/**/*.cy.*"],
  "compilerOptions": {
    "rootDirs": ["src", "../types"],
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["../types/*"],
    }
  }
}
